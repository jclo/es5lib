{
  "name": "es5lib",
  "version": "0.0.1",
  "description": "A template for writing large ES5 Javascript libraries",
  "main": "index.js",
  "bin": {
    "es5lib": "./bin/es5lib.js"
  },
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "app": "gulp connect",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "report": "nyc report",
    "precommit": "npm run makedist && npm run test && npm run check-coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/es5lib.git"
  },
  "keywords": [
    "UMD",
    "library",
    "template",
    "boilerplate",
    "instantiation patterns",
    "functional instantiation",
    "functional-shared instantiation",
    "prototypal instantiation",
    "pseudoclassical instantiation",
    "pseudoclassical auto instantiation"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/es5lib/issues"
  },
  "homepage": "https://github.com/jclo/es5lib",
  "dependencies": {
    "nopt": "4.0.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "browserify": "16.1.0",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "del": "3.0.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-connect": "5.5.0",
    "gulp-footer": "2.0.1",
    "gulp-header": "2.0.1",
    "gulp-replace": "0.6.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.0",
    "husky": "0.14.3",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "open": "0.0.5",
    "opener": "1.4.3",
    "require-dir": "1.0.0",
    "run-sequence": "2.2.1",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.10.0"
  }
}
